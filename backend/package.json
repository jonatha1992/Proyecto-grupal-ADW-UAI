{
  "name": "turnosmed-backend",
  "version": "1.0.0",
  "description": "Backend API para TurnosMed - Sistema de gestión de turnos médicos",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "build:watch": "tsc -w",
    "postinstall": "npm run build",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "api",
    "express",
    "typescript",
    "postgresql",
    "turnos",
    "medicos"
